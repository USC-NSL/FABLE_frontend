{
    "manifest_version": 3,
    "name": "FABLE frontend",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "webNavigation"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "extension_thumbnail.png"
    }
  }
  