{
    "manifest_version": 3,
    "name": "FABLE frontend",
    "description": "Base Level Extension",
    "version": "1.1",
    "permissions": [
      "webRequest",
      "scripting",
      "tabs",
      "notifications"
    ],
    "host_permissions": [
      "*://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "19": "1.png",
        "38": "2.png"
      }
    },
    "web_accessible_resources": [
      {
          "resources": ["brokenlink.png"],
          "matches": ["<all_urls>"]

      }
    ]
  }
  