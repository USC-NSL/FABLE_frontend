{
    "manifest_version": 3,
    "name": "FABLE frontend",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "storage",
      "webRequest",
      "webNavigation",
      "scripting",
      "tabs",
      "notifications"
    ],
    "host_permissions": [
      "*://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  